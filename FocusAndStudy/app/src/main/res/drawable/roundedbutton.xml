<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="45dp" android:left="45dp" android:right="45dp" android:bottom="45dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#1F5F1C" />
            <corners android:bottomRightRadius="1000dp"
                android:bottomLeftRadius="1000dp"
                android:topRightRadius="1000dp"
                android:topLeftRadius="1000dp"/>
        </shape>
    </item>
    <item android:top="35dp" android:left="35dp" android:right="35dp" android:bottom="35dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" >
            <stroke android:width="2dp" android:color="#36A556"/>
            <corners android:bottomRightRadius="1000dp"
                android:bottomLeftRadius="1000dp"
                android:topRightRadius="1000dp"
                android:topLeftRadius="1000dp"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" >
            <stroke android:width="2dp" android:color="#36A556"/>
            <corners android:bottomRightRadius="1000dp"
                android:bottomLeftRadius="1000dp"
                android:topRightRadius="1000dp"
                android:topLeftRadius="1000dp"/>
        </shape>
    </item>
    <item
        android:top="80dp"
        android:bottom="80dp"
        android:right="80dp"
        android:left="80dp"
        android:drawable="@drawable/play" />
</layer-list>



